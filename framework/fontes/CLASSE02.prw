Method Gravacao(nOpcao) Class SaldoInicial
	Local lRetorno		:= .T.

	Private	lMsErroAuto	:= .F.

	DbSelectArea("SB9")
	DbSetOrder(1)

	If DbSeek(xFilial("SB9") + ::cProduto + ::cArmazem)
		lRetorno	:= .F.
		::cMensagem	:= "Saldo Inicial jแ cadastrado " + CRLF + CRLF + "Produto : " + ::cProduto + CRLF + "Armazem : " + ::cArmazem	+ CRLF
	Else
		::AddValues("B9_FILIAL", xFilial("SB9"))

		MSExecAuto({|x, y| MATA220(x, y)}, ::aValues, nOpcao)

		If lMsErroAuto
			lRetorno := .F.

			If ::lExibeTela
				MostraErro()
			EndIf
			
			If ::lGravaLog
				::cMensagem := MostraErro(::cPathLog, ::cFileLog)
			EndIf
		EndIf
	EndIf

Return lRetorno
