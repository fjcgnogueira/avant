Method AddValues(cCampo, xValor) Class SaldoInicial

	If AllTrim(cCampo) == "B9_COD"
		::cProduto	:= xValor
	ElseIf AllTrim(cCampo) == "B9_LOCAL"
		::cArmazem	:= xValor
	ElseIf AllTrim(cCampo) == "B9_QINI"
		::nQIni		:= xValor
	ElseIf AllTrim(cCampo) == "B9_QISEGUM"
		::nQIniSeg	:= xValor
	EndIf

	_Super:AddValues(cCampo, xValor)

Return Nil
